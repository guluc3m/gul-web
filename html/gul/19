var align = {
    "v1" : "right", 
    "v2" : "right", 
    "b" : "left", 
    "e1" : "center", 
    "e2" : "left", 
    "c" : "center"
}

var colors = {
    "v" : "#fff", 
    "b" : "#0af", 
    "e" : "#f8f",
    "c" : "#ddf"
}

var slide = 0;

function proclaim(backgorund, text) {
  $('ventext').val('text')
}

function impetus() {
  s = script[slide];
  picture = s[0];
 
  $('#vendetta').css("background-image", "url(/img/gul19/" + s[0] + ".png)");
  $('#ventext').text(s[1]).css("color", colors[picture[0]]).css("text-align", align[picture]);
  slide = slide + 1;
}

$('#vendetta').click(impetus);
impetus();
</script>

    <script type="text/javascript">
      function showSubscribeForm() {
        $('#botonaco').slideUp();
        $('#mail').click(function() {
          if ($('#mail').hasClass("inputlight")) {
            $('#mail').removeClass("inputlight").addClass("inputstrong");
            $('#mail').val("");
          }
        });
      }
        
      var expanded = false;
      function showMainStory() {
        toSize = expanded ? 550 : 1000;
        expanded = !expanded;
        $("#mainstory .body").animate({
          "max-height": "" + toSize + "px"
        }, 1000);
      }
    </script>
    
    <script type="text/javascript">
     // google analytics
     var _gaq = _gaq || [];
     _gaq.push(['_setAccount', 'UA-65510-9']);
     _gaq.push(['_setDomainName', 'gul.es']);
     _gaq.push(['_setAllowLinker', true]);
     _gaq.push(['_trackPageview']);
     (function() {
       var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
       ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
       var s = document.getElementsByTagName('script')[0];
       s.parentNode.insertBefore(ga, s);
     })();
    </script>
    
    <script type="text/javascript">
      function annrefresh() {
        $.ajax({
            url: "/announce.txt", 
        }).done(function(data) {
            $("#announce").html(data);
        });
      }
      setInterval (annrefresh, 5000);
    </script>
  
